<p style="color: green"><%= notice %></p>

<h1>Products</h1>

<div id="products">
  <% @products.each do |product| %>
    <%= render product %>
    <p>
      <%= link_to "Show this product", users_product_path(product) %>
    </p>
  <% end %>
</div>

<%= link_to "New product", new_users_product_path %>
