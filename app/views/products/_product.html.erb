<div id="<%= dom_id product %>">
  <p>
    <strong>User:</strong>
    <%= product.user_id %>
  </p>

  <p>
    <strong>Category:</strong>
    <%= product.category_id %>
  </p>

  <p>
    <strong>Status:</strong>
    <%= product.status %>
  </p>

  <p>
    <strong>Name:</strong>
    <%= product.name %>
  </p>

  <p>
    <strong>Short description:</strong>
    <%= product.short_description %>
  </p>

  <p>
    <strong>Long description:</strong>
    <%= product.long_description %>
  </p>

  <p>
    <strong>Age:</strong>
    <%= product.age %>
  </p>

  <p>
    <strong>Sex:</strong>
    <%= product.sex %>
  </p>

  <p>
    <strong>Breed:</strong>
    <%= product.breed %>
  </p>

  <p>
    <strong>Height:</strong>
    <%= product.height %>
  </p>

  <p>
    <strong>veigth:</strong>
    <%= product.veigth %>
  </p>

  <p>
    <strong>Teeth:</strong>
    <%= product.teeth %>
  </p>

  <p>
    <strong>Castrated:</strong>
    <%= product.castrated %>
  </p>

  <p>
    <strong>Price:</strong>
    <%= product.price %>
  </p>

  <p>
    <strong>Quantity:</strong>
    <%= product.quantity %>
  </p>

  <p>
    <strong>City:</strong>
    <%= product.city %>
  </p>

  <p>
    <strong>Country:</strong>
    <%= product.country %>
  </p>

  <p>
    <strong>State:</strong>
    <%= product.state %>
  </p>

  <p>
    <strong>Address:</strong>
    <%= product.address %>
  </p>

  <p>
    <strong>Self stock:</strong>
    <%= product.self_stock %>
  </p>

  <p>
    <strong>Verified:</strong>
    <%= product.verified ? 'Yes' : 'No' %>
  </p>

</div>
