
<div id="<%= dom_id service %>">
  <p>
    <strong>Title:</strong>
    <%= service.title %>
  </p>

  <p>
    <strong>Content:</strong>
    <%= raw service.content %>
  </p>

  <p>
    <strong>Cover Image:</strong>
    <%= image_tag url_for(service.cover_image), alt: service.title, class: "icon-shape icon-md" if service.cover_image.attached? %>
  </p>


</div>
